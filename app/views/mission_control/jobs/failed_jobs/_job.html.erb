<tr class="job">
  <td>
    <%= job.class_name %>
    (<%= job.serialized_arguments.inspect %>)
  </td>
  <td>
    <%= job.last_execution_error.error_class %>:
    <%= job.last_execution_error.message %>
  </td>
  <td>
    <%= render "mission_control/jobs/failed_jobs/actions", job: job %>
  </td>
</tr>
